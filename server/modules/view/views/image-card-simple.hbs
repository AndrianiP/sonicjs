<div class="row">
{{#each list}}
  <div class="col text-{{../data.textJustification}}">
    <img class="img-fluid" src="{{this.data.imageUrl}}" />
    <h3>{{this.data.title}}</h3>
    <a target="_blank" href="/admin/content/edit/fe/{{this.id}}"><i class="bi bi-pencil"></i></a>
    {{{this.data.body}}}
  </div>
{{/each}}
</div>