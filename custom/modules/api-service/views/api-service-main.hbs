{{#each data.services}}
  <h3>{{this.title}}</h3>
  <div class="alert alert-secondary" role="alert">
    [root]/server/services/{{this.slug}}.service.js  </div>
  <p>{{this.description}}</p>
  <a href="/docs/developer-guide/api-backend/{{this.slug}}-service" class="btn btn-primary btn-medium">
    <i class="bi bi-box-arrow-in-down-right me-2"></i>
    {{this.title}} Details</a>
{{/each}}